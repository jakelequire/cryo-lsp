{
  "name": "cryo-vscode-extension",
  "displayName": "Cryo Language Support",
  "description": "Provides support for the Cryo programming language",
  "version": "0.0.1",
  "publisher": "Jake LeQuire",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakelequire/cryo-lsp"},
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:cryo"
  ],
  "main": "out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "cryo",
        "aliases": [
          "Cryo",
          "cryo"
        ],
        "extensions": [
          ".cryo"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "typescript": "^5.5.2",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^9.0.1",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  }
}
